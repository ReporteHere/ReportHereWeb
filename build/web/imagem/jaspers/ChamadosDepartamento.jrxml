	<parameter name="idTipo" class="java.lang.Long">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT assunto, descricao, t.nome, to_char(d.data, 'DD/MM/YYYY') as data_criacao,
(select count(*) from ocorrencia o inner join departamento_ocorrencia d on o.id = d.ocorrencia_fk
inner join usuario u on u.id = d.usuario_fk
inner join tipo_ocorrencia t on t.id = o.tipo_ocorrencia_fk
inner join tipo_usuario tu on tu.id = u.tipo_usuario_fk where tu.id = $P{idTipo}) as qtde, tu.nome as departamento
FROM ocorrencia o
inner join departamento_ocorrencia d on o.id = d.ocorrencia_fk
inner join usuario u on u.id = d.usuario_fk
inner join tipo_ocorrencia t on t.id = o.tipo_ocorrencia_fk
inner join tipo_usuario tu on tu.id = u.tipo_usuario_fk
where tu.id = $P{idTipo}
order by 4,3 asc]]>
	</queryString>
